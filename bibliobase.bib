@article{chhavisodhi2014,
	Abstract = {Universalising health coverage is the current goal
                  of the health service system in India. Tax-funded
                  insurance for poor families is the method chosen for
                  attaining this objective. The Rashtriya Swasthya
                  Bima Yojana was rolled out in 2008 for households
