@Article{dhawan1997,
  Title =        {Production Benefits from Large-Scale-Canal
                  Irrigation},
  Author =       {B D Dhawan},
  Journal =      {Economic and Political Weekly},
  Year =         1997,
  Month =        {Dec},
  Number =       52,
  Volume =       32,
  Abstract =     {This paper aims to demonstrate how National Accounts
                  Statistics compiled by the CSO can be tapped by
                  researchers to obtain nationwide insights into costs
                  and returns aspects of irrigation activity,
                  especially public canal irrigation. Two noteworthy
                  research results are: (1) income benefits from canal
                  waters prima facie were more than the supply cost of
                  such waters throughout the period 1980-81 to 1992-93
                  and (2) the supply cost of canal irrigation tended
                  to rise much faster than the income benefits,
                  probably because hidden costs of corruption and
                  sheer inefficiencies in canal development and
                  administration soared in the above period of general
                  erosion in ethics and public accountability. },
  Date-added =   {2014-02-15 16:33:27 +0000},
  Date-modified ={2014-09-09 00:55:34 +0000},
  Keywords =     {irrigation, canal},
  Publisher =    {Economic and Political Weekly},
  Type =         {Journal}
}

