@article{mullertagtow2009,
  author =       {Muller, Mark and Tagtow, Angie and Roberts, Susan
                  L. and MacDougall, Erin},
  title =        {Aligning Food Systems Policies to Advance Public
                  Health},
  journal =      {Journal of Hunger \& Environmental Nutrition},
  volume =       4,
  number =       {3-4},
  pages =        {225-240},
  year =         2009,
  doi =          {10.1080/19320240903321193},
  note =         {PMID: 23144671},
  URL =          { http://dx.doi.org/10.1080/19320240903321193 },
  abstract =     { The involvement of public health professionals in
                  food and agricultural policy provides tremendous
                  opportunities for advancing the public's health. It
                  is particularly challenging, however, for
                  professionals to understand and consider the
                  numerous policy drivers that impact the food system,
                  which range from agricultural commodity policies to
                  local food safety ordinances. Confronted with this
                  complexity in the food system, policy advocates
                  often focus on narrow objectives with disregard for
                  the larger system. This commentary contends that, in
                  order to be most effective, public health
                  professionals need to consider the full range of
                  interdependent policies that affect the system. Food
                  policy councils have proven to be an effective tool,
                  particularly at the local and state level, for
                  developing comprehensive food systems policies that
                  can improve public health. }
}

