@Article {bhaskarbrick2015,
	Title 				= {Forced Migration of Labourers to Brick Kilns in Uttar Pradesh},
	Author 				= {Bhaskar Majumder},
	Journal 			= {Economic and Political Weekly},
	Year 				= {2015},
	Month 				= {Jun},
	Number 				= {26-27},
	Volume 				= {50},
	Abstract 			= {Seasonal employment in the rural economy at a wage rate below subsistence level forces underprivileged labourers to migrate for survival. Brick kilns in India are a major destination for migrant labourers, who are tied to them for the production season after accepting advance wages from agents. Based on the livelihood conditions of the migrant labourers and their indebtedness, this paper points out these migrations have to be seen as forced. The labourers remain in inter-kiln circulation, but are prevented from moving on because of their limited skills and social networks. They return home to repay a never-ending debt and again receive advance wages for the next season--renewing a cycle of debt and migration for survival.
},
	Publisher 			= {Economic and Political Weekly},
	Type 				= {Journal},
  }

