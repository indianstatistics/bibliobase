@incollection{NApulses142_agostini1988trends,
  Abstract =     {World pulse production and utilization declined
                  during the 1970s, reflecting a reduction in the area
                  sown to pulses in the developed countries and
                  smaller yields in developing countries, most of
                  which gave priority to cereals. With supply lagging
                  behind demand, prices increased and consumption
                  dropped sharply during the decade, particularly in
                  the developing countries where pulses are a major
                  source of protein. By the end of the 1970s, however,
                  higher prices had begun to stimulate pulse
                  production in many countries, a trend which
                  continued in the early 1980s. Looking to the future,
                  global output may continue to grow only slowly until
                  the end of the current decade, and perhaps even
                  beyond, reflecting mainly the limited attention by
                  governments to this sector.},
  author =       {Agostini, Barbara Bennett and Khan, Doud},
  Booktitle =    {World crops: Cool season food legumes},
  pages =        {461--482},
  Publisher =    {Springer},
  title =        {Trends, Situation and Outlook for the World Pulse
                  Economy},
  year =         1988
}

