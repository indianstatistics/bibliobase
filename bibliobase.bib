@Article{jayan2014,
  Title =        {The Demographic Challenge and Employment Growth in
                  {I}ndia},
  Author =       {Jayan Jose Thomas},
  Journal =      {Economic and Political Weekly},
  Year =         2014,
  Month =        {Feb},
  Number =       6,
  Volume =       49,
  Abstract =     {The working-age population of {{I}ndia} is growing
                  in size, the labour force is shifting away from
                  agriculture and, with higher education, workers are
                  also seeking better-quality non-agricultural
                  jobs. However, the trends between 2004-05 and
                  2011-12 indicate that employment generation in the
                  country has been inadequate to meet this
                  challenge. Construction has virtually become the
                  only source of incremental employment in rural
                  {I}ndia. In the urban areas, men have been able to
                  obtain a disproportionate share of high-productivity
                  employment. },
