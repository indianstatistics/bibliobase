@article{msvikasbtcotton2011,
	abstract = {This note examines costs and returns from the
                  cultivation of different types of cotton in a
                  rainfed village in the Vidarbha region of
                  Maharashtra, {I}ndia. While the pros and cons of GM
                  cotton are extensively debated, there are only a few
                  empirical studies on the economic performance of Bt
                  cotton, particularly under rainfed conditions. The
                  results from a detailed survey of farm business
                  incomes show that Bt cotton was a clear leader in
                  terms of production and gross value of output when
                  grown as a stand-alone crop. However, on the fields
                  of small and marginal farmers, where cotton was
                  usually intercropped with sorghum (or other cereals
                  and pulses), the relative income advantage of Bt
                  cotton declined. Further, expenditure on chemical
                  pesticides was higher for Bt cotton than for other
                  varieties of cotton. Variability in production was
                  also higher for Bt cotton than for other types of
                  cotton.},
	author = {Swaminathan, Madhura and Rawal, Vikas},
	date-modified = {2014-09-24 07:21:02 +0000},
	journal = {Review of Agrarian Studies (RAS)},
	keywords = {GM; Bt cotton; costs of cultivation; income; village; Maharashtra; {I}ndia},
	month = jan,
	number = 1,
	pages = {101-124},
	title = {Are there Benefits from the Cultivation of Bt Cotton? A Comment Based on Data from a Vidarbha Village},
	url = {http://ras.org.in/are_there_benefits_from_the_cultivation_of_bt_cotton_a_comment_based_on_data_from_a_vidarbha_village},
	volume = 1,
	year = 2011,
	bdsk-url-1 = {http://ras.org.in/are_there_benefits_from_the_cultivation_of_bt_cotton_a_comment_based_on_data_from_a_vidarbha_village}}

